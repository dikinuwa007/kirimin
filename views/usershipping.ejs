<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=tab, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <!-- <a href="/user/:iduser/shipping/add">Add shipping</a> -->
    <a href="/user/<%= idProfile %>/shipping/add">Add Shipping</a>
    <table border="1">
        <tr>
            <th>destination</th>
            <th>estArrival</th>
            <th>createdAt</th>
            <th>name</th>
            <th>price</th>
            <th>description</th>
            <!-- <th>status</th> -->
        </tr>
        <% data.forEach(e => { %>
        <tr>
            <td><%= e.destination %></td>
            <td><%= formatDate(e.estArrival) %></td>
            <td><%= formatDate(e.createdAt) %></td>
            <% e.ShippedItems.forEach(element => { %>
            <td><%= element.Item.name %></td> <br>
            <td><%= element.Item.price %></td>
            <td><%= element.Item.description %></td>
            <% }) %>
        </tr>
        <% }) %>
    </table>
</body>
</html>